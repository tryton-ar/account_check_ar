<?xml version="1.0"?>
<form>
    <label name="bank"/>
    <field name="bank"/>
    <label name="currency"/>
    <field name="currency"/>
    <label name="name"/>
    <field name="name"/>
    <label name="amount"/>
    <field name="amount"/>
    <label name="date_in"/>
    <field name="date_in"/>
